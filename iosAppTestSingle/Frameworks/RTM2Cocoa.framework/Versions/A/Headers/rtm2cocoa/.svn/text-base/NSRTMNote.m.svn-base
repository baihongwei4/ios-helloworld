#import "NSRTMNote.h"


@implementation NSRTMNote

-(NSString *)getValue {
	return value;
}
-(NSString *)getID {
	return note_id;
}

-(NSDate *)getCreated {
	return [NSDate dateFromISO8601:created];
}
-(NSDate *)getModified {
	return [NSDate dateFromISO8601:modified];
}
-(NSString *)getTitle {
	return title;
}

@end
